<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from ServiceService WSDL service -->

  <service name="Service" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://connect.com"/>
      <parameter name="wsdlServiceElement" value="ServiceService"/>
      <parameter name="schemaQualified" value="http://connect.com"/>
      <parameter name="wsdlServicePort" value="Service"/>
      <parameter name="className" value="com.connect.ServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Service"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="signIn" qname="operNS:signIn" xmlns:operNS="http://connect.com" returnQName="retNS:signInReturn" xmlns:retNS="http://connect.com" returnType="rtns:Client" xmlns:rtns="http://connect.com" soapAction="" >
        <parameter qname="pns:username" xmlns:pns="http://connect.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:password" xmlns:pns="http://connect.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="signUp" qname="operNS:signUp" xmlns:operNS="http://connect.com" returnQName="retNS:signUpReturn" xmlns:retNS="http://connect.com" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:firstname" xmlns:pns="http://connect.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:lastname" xmlns:pns="http://connect.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:username" xmlns:pns="http://connect.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:password" xmlns:pns="http://connect.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="signIn signUp"/>

      <typeMapping
        xmlns:ns="http://connect.com"
        qname="ns:Client"
        type="java:com.connect.Client"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
